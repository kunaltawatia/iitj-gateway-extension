{
  "name": "Gateway",
  "description": "Forticlient Gateway Extension",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "declarativeContent",
    "activeTab",
    "scripting",
    "tabs",
    "tabGroups"
  ],
  "host_permissions": [
    "https://*/*"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}